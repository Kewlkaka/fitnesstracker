<!-- userWorkoutDetails.ejs -->

<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <!--<link rel="stylesheet" href="styles.css">-->
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
  <title>FITNESS TRACKER API - Exercise-Workout</title>
</head>
<body style="background-color: #222222; color: white;">
  <h1 class="text-center">Fitness Tracker - Exercise and Workout Details</h1>
  <table class="table table-dark">
    <thead>
      <tr>
          <th>Exercise Name</th>
          <th>Sets</th>
          <th>Reps</th>
          <th>Weight</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach((row) => { %>
        <tr>
          <td><%= row.exercise_name %></td>
          <td><%= row.sets%></td>
          <td><%= row.reps %></td>
          <td><%= row.weight %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
